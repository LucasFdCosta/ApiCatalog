{
  "ConnectionStrings": {
    "DefaultConnection": "MyConnStringGoesHere"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
"JWT": {
    "ValidAudience": "{{myhostgoeshere}}",
    "ValidIssuer": "{{myhostgoeshere}}",
    "SecretKey": "{{mysecregoeshere}}",
    "TokenValidityInMinutes": 30,
    "RefreshTokenValidityInMinutes": 60
  },
  "AllowedHosts": "*",
  "MyRateLimit": {
    "PermitLimit": 9,
    "Window": 9,
    "ReplenishmentPeriod": 1,
    "QueueLimit": 2,
    "SegmentsPerWindow": 4,
    "TokenLimit": 8,
    "TokenLimit2": 12,
    "TokensPerPeriod": 4,
    "AutoReplenishment": true
  }
}
